<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>ZG</Authors>
    <Description>模型结果：API统一返回结果</Description>
    <Copyright>VirtualUniverse</Copyright>
    <PackageReleaseNotes></PackageReleaseNotes>
    <Version>1.0.3</Version>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <EnableNETAnalyzers>true</EnableNETAnalyzers>
	  <PackageProjectUrl>https://github.com/zhengganCN/VirtualUniverse.Util/tree/master/VirtualUniverse.ModelResultStandard</PackageProjectUrl>
	  <PackageLicenseFile>LICENSE</PackageLicenseFile>
	  <PackageIcon>tool.png</PackageIcon>
